  </xs:complexType>
  <xs:complexType name="GetCourseDetailsResponseType">
    <xs:sequence>
      <xs:element type="cour:CourseDetailsType" name="CourseDetails" xmlns:cour="http://in28minutes.com/courses"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>