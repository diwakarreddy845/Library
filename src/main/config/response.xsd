<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://in28minutes.com/courses"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
